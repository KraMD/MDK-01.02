using namespace std;
#include <iostream>
#include <math.h>
main ()
{
	float a,b,c,d,e,g,h,t,m,n,q,p,s;
	cout<<"vvedite x1=";
	cin>>e;
	
		cout<<"vvedite y1=";
		cin>>g;
	
			cout<<"vvedite x2=";
			cin>>h;
	
				cout<<"vvedite y2=";
				cin>>t;
	
	n=h-e;
	m=t-g;
	a=abs(n);
	b=abs(m);
		p=2;
		q=pow(a,p);
		d=pow(b,p);
		s=q+d;
		c=sqrt(s);
			cout<<"c="<<c;
			cout<<"\n ";
	system("pause");
	return 0;
	}
	
	
